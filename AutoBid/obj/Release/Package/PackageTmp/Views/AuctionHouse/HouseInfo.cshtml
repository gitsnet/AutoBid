@model AutoBid.Models.Auction.AuctionHouseModel
@{
    ViewBag.Title = "HouseInfo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="section">

    <div class="container">

        @Html.Partial("_AuctionHouseVehicleMenu")

        <div class="row">
            <div class="col-md-9">

                <div class="row auction_house_content">
                    <h1>House Info</h1>

                    <div class="row">
                        <div class="col-md-5">
                            @Html.HiddenFor(m => m.AuctionHouseID, new { @id = "hdnAuctionHouseID" })
                            <table width="100%" border="0" cellspacing="0" cellpadding="0" class="table">
                                <tr>

                                    <td contenteditable="false" id="tdAucHouseName">
                                        <strong id="strHouseName">@Model.AuctionHouseName</strong>

                                    </td>

                                    <td>&nbsp;<button class="btn btn-primary btn-xs" id="editbtnAucHouseName">Edit</button></td>
                                    <td><button class="btn btn-primary btn-xs" id="savebtnAucHouseName" style="display:none;">Save</button></td>
                                    <td><button class="btn btn-primary btn-xs" id="cancelbtnAucHouseName" style="display:none;">Cancel</button></td>

                                </tr>
                                <tr>
                                    <td><strong>Description</strong></td>
                                    <td>&nbsp;<button class="btn btn-primary btn-xs" id="editbtnAucHouseDescrip">Edit</button></td>
                                    <td><button class="btn btn-primary btn-xs" style="display:none;" id="savebtnAucHouseDescrip">Save</button></td>
                                    <td><button class="btn btn-primary btn-xs" id="cancelbtnAucHouseDescrip" style="display:none;">Cancel</button></td>
                                </tr>
                                <tr>
                                    <td colspan="2" contenteditable="false" id="tdAucHouseDescrip">
                                        @Model.About

                                    </td>
                                </tr>

                            </table>

                        </div>

                        <div class="col-md-7">

                            <table width="100%" border="0" cellspacing="0" cellpadding="0" class="table table-striped">
                                <tr>
                                    <td contenteditable="false">Advertised Buyers Fees</td>
                                    <td contenteditable="false" id="tdAucHouseBuyeerFee"><strong id="strHouseBuyeerFee">@Model.Buyer_Fees</strong></td>
                                    <td><button class="btn btn-primary btn-xs" id="editbtnAucHouseBuyeerFees">Edit</button></td>
                                    <td><button class="btn btn-primary btn-xs" style="display:none;" id="savebtnAucHouseBuyeerFees">Save</button></td>
                                    <td><button class="btn btn-primary btn-xs" style="display:none;" id="cancelbtnAucHouseBuyeerFees">Cancel</button></td>
                                </tr>
                                <tr>
                                    <td contenteditable="false">Usual Sale Dates</td>
                                    <td contenteditable="false" id="tdAucSaleDate"><strong id="strSaleDate">@Model.SaleDate</strong></td>
                                    <td><button class="btn btn-primary btn-xs" id="editbtnSaleDate">Edit</button></td>
                                    <td><button class="btn btn-primary btn-xs" style="display:none;" id="savebtnAucHouseSaleDate">Save</button></td>
                                    <td><button class="btn btn-primary btn-xs" style="display:none;" id="cancelbtnAucHouseSaleDate">Cancel</button></td>
                                </tr>
                                <tr>
                                    <td>Location</td>
                                    <td contenteditable="false" id="tdAucHouseAddress"><strong id="strHouseAddress">@Model.Address</strong></td>
                                    <td><button class="btn btn-primary btn-xs" id="editbtnAucHouseAddress">Edit</button></td>
                                    <td><button class="btn btn-primary btn-xs" style="display:none;" id="savebtnAucHouseAddress">Save</button></td>
                                    <td><button class="btn btn-primary btn-xs" style="display:none;" id="cancelbtnAucHouseAddress">Cancel</button></td>
                                </tr>
                                <tr>
                                    <td>Contact Details</td>
                                    <td contenteditable="false" id="tdAucHouseContact"><strong id="strHouseContact">@Model.ContactNo</strong></td>
                                    <td><button class="btn btn-primary btn-xs" id="editbtnAucHouseContact">Edit</button></td>
                                    <td><button class="btn btn-primary btn-xs" style="display:none;" id="savebtnAucHouseContact">Save</button></td>
                                    <td><button class="btn btn-primary btn-xs" style="display:none;" id="cancelbtnAucHouseContact">Cancel</button></td>
                                </tr>
                                <tr>
                                    <td>Sales Commission</td>
                                    <td contenteditable="false" id="tdAucHouseSaleComm">&nbsp;<strong id="strAucHouseSaleComm">@Model.SalesCommission</strong></td>
                                    <td><button class="btn btn-primary btn-xs" id="editbtnAucHouseSaleComm">Edit</button></td>
                                    <td><button class="btn btn-primary btn-xs" style="display:none;" id="savebtnAucHouseSaleComm">Save</button></td>
                                    <td><button class="btn btn-primary btn-xs" style="display:none;" id="cancelbtnAucHouseSaleComm">Cancel</button></td>
                                </tr>
                                <tr>
                                    <td>Web Address</td>
                                    <td class="tdAuction" contenteditable="false" id="tdAucHouseWebSite"><strong id="strHouseWebSite">@Model.WebSite</strong></td>
                                    <td><button class="btn btn-primary btn-xs" id="editbtnAucHouseWebSite">Edit</button></td>
                                    <td><button class="btn btn-primary btn-xs" style="display:none;" id="savebtnAucHouseWebSite">Save</button></td>
                                    <td><button class="btn btn-primary btn-xs" style="display:none;" id="cancelbtnAucHouseWebSite">Cancel</button></td>
                                </tr>

                                <tr>
                                    <td>Contact Email</td>
                                    <td class="tdEmail" contenteditable="false" id="tdAucEmail"><strong id="strAucEmail">@Model.Email</strong></td>
                                    <td><button class="btn btn-primary btn-xs" id="editbtnAucHouseEmail">Edit</button></td>
                                    <td><button class="btn btn-primary btn-xs" style="display:none;" id="savebtnAucHouseEmail">Save</button></td>
                                    <td><button class="btn btn-primary btn-xs" style="display:none;" id="cancelbtnAucHouseEmail">Cancel</button></td>
                                </tr>

                                <tr>

                                    <td >Logo</td>
                                   
                                    <td id="tdAucHouseLogo">
                                        <img src="/Content/Assets/AuctionHouseImages/@Model.Logo">
                                        <button class="btn btn-primary btn-xs" style="display:none;" id="savebtnAucHouseLogo">Save</button>
                                        @*<button class="btn btn-primary btn-xs" style="display:none;" id="cancelbtnAucHouseLogo">Cancel</button>*@
                                    </td>


                                    <td><button class="btn btn-primary btn-xs" id="editbtnAucHouseLogo">Edit</button></td>
                                 
                                </tr>




                            </table>


                        </div>


                    </div>

                </div>

            </div>

            <div class="col-md-3">

                <div class="adbanner">
                    <span>Ad Banner</span>
                </div>

            </div>

        </div>
        <!-- /.row -->

    </div>
    <!-- /.container -->

</div>
<script type="text/javascript" src="~/Content/js/jquery.form.js"></script>
