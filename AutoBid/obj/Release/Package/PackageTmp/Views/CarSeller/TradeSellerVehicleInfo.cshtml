                                    <div class="col-md-4">
                                        @*<input type="text" placeholder="location" class="form-control" id="carlocation">*@
                                        @Html.TextBoxFor(m => m.CarLocation, new { @class = "form-control", @placeholder = "Location" })
                                        @Html.ValidationMessageFor(m => m.CarLocation)
                                    </div>
                                    <div class="col-md-4"> <a class="underline" href="javascript:void(0)">Find</a> </div>
                                </div>

                                <div class="form-group">
