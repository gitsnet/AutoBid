                                    <div class="col-md-4">
                                        @Html.TextBoxFor(m => m.ContactNumberOnAdvert, new { @class = "form-control", @placeholder = "Contact Number On Advert" })
                                        @Html.ValidationMessageFor(m => m.ContactNumberOnAdvert)
                                    </div>
                                    <div class="col-md-4"></div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="cNo2">@*Contact Number*@@Html.DisplayNameFor(m => m.ContactNumber) </label>
                                    <div class="col-md-4">

                                        @Html.TextBoxFor(m => m.ContactNumber, new { @class = "form-control", @placeholder = "Contact Number" })
                                        @Html.ValidationMessageFor(m => m.ContactNumber)
                                    </div>
                                    <div class="col-md-4"></div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-4 control-label" for="inputPassword3">&nbsp;</label>
                                <div class="col-sm-8">
                                    <button class="btn btn-primary btn-lg active right" id="btnTabDetail2" type="button">Next</button>
                                    <button class="btn btn-primary btn-lg active left PreviousBtn" type="button" id="TabPrev1" style="float:right;margin-right:10px;">Previous</button>

                                </div>
                            </div>
                        </form>
                    </div>
                </div>
