
@model AutoBid.Models.CarSeller.CarSellerVehicleInfoModel

@{
    ViewBag.Title = "Trade Seller Vehicle Info";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .buttonClass {
        background-color: #011a2d;
        border: 0 none;
        color: #fff;
        padding: 10px 15px;
    }

        .buttonClass:hover, .buttonClass:focus, .buttonClass.active {
            background-color: #19507a;
            height: auto;
        }
</style>

<div class="section">
    <div class="container">
        <div class="row auction_house">
            <ul class="nav nav-pills nav-justified">
                <li class="active" id="liDetails"><input data-value="#tab1" type="button" value="Details" name="details" id="details" class="buttonClass active"></li>
                    <li id="liDetails2"><input type="button" data-value="#tab2" id="details2" name="details2" value="Details 2" class="buttonClass" disabled></li>
                    <li id="liPhotoUpload"><input type="button" data-value="#tab3" id="btnPhotoUpload" name="btnPhotoUpload" value="Photo Upload" class="buttonClass" disabled></li>
                    <li id="liPreview"><input type="button" id="btnPreview" data-value="#tab4" name="btnPreview" value="Preview" class="buttonClass" disabled></li>
                    <li id="liConfirm"><input type="button" id="btnConfirm" data-value="#tab5" name="btnConfirm" value="Confirm" class="buttonClass" disabled></li>

                @*<li class="active" id="liDetails"><input data-value="#tab1" type="button" value="Details" name="details" id="details" class="buttonClass active"></li>
                <li id="liDetails2"><input type="button" data-value="#tab2" id="details2" name="details2" value="Details 2" class="buttonClass"></li>
                <li id="liPhotoUpload"><input type="button" data-value="#tab3" id="btnPhotoUpload" name="btnPhotoUpload" value="Photo Upload" class="buttonClass"></li>
                <li id="liPreview"><input type="button" id="btnPreview" data-value="#tab4" name="btnPreview" value="Preview" class="buttonClass"></li>
                <li id="liConfirm"><input type="button" id="btnConfirm" data-value="#tab5" name="btnConfirm" value="Confirm" class="buttonClass"></li>*@
            </ul>
        </div>
        <div class="row">
            <div id="tab1">
                <div class="col-md-8 formbg1 mb20">

                    <div class="sellerfg_header sellmycarForm"> Sell My Car </div>
                    <div class="row formarea">
                        <form id="FormCarSellingDetails" class="form-horizontal" method="post" action="/CarSeller/CarSellingRegistration" novalidate="novalidate">
                            <div class="highlight">
                                <div class="form-group">
                                    <label class="col-sm-4 control-label" for="saleDate">Title</label>
                                    <div class="col-sm-4">
                                        @Html.TextBoxFor(m => m.Title, new { @class = "form-control" })
                                        @Html.ValidationMessageFor(m => m.Title)
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-4 control-label" for="saleDate">Registration Number</label>
                                    <div class="col-sm-4">
                                        @Html.TextBoxFor(m => m.RegistrationNumber, new { @class = "form-control inputRegNo inputRegNo2ndstsp" })
                                        @Html.ValidationMessageFor(m => m.RegistrationNumber)
                                    </div>

                                </div>
                                
                            </div>
                            <div class="highlight">
                                <div class="row auction_house_content formHighlightheader"><h1>Vehicle Details</h1></div>

                                <div class="form-group">
                                    <label for="inputPassword3" class="col-sm-4 control-label">@Html.DisplayNameFor(m => m.MakeID)</label>
                                    <div class="col-sm-8">
                                        @Html.DropDownList("MakeID", new SelectList(Model.MakeList, "ID", "Makename"), "--Select--", new { @class = "form-control", @id = "ddlMakes" })
                                        @Html.TextBoxFor(m => m.NewCarMakeName, new { @id = "txtNewMakename", @style = "display:none;", @class = "form-control" })
                                        @Html.ValidationMessageFor(m => m.MakeID)
                                        <span class="linktext" id="spnCarMakeNotInList">My car make not in list.<br></span>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="inputPassword3" class="col-sm-4 control-label">@Html.DisplayNameFor(m => m.ModelID)</label>
                                    <div class="col-sm-8">
                                        @Html.HiddenFor(m => m.ModelID, new { @id = "hdnModelID" })
                                        @Html.DropDownList("ModelID", new SelectList(Model.CarModelList, "ID", "Modelname"), "--Select--", new { @class = "form-control", @id = "ddlModels" })

                                        <span id="spnCarModelNotInList" class="linktext">My car model not in list.</span>
                                        @Html.TextBoxFor(m => m.NewCarModelName, new { @id = "txtNewModelname", @style = "display:none;", @class = "form-control" })
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="inputPassword3" class="col-sm-4 control-label">@Html.DisplayNameFor(m => m.BodyTypeID)</label>
                                    <div class="col-sm-8">
                                        @Html.DropDownList("BodyTypeID", new SelectList(Model.BodyTypeList, "ID", "Type"), "--Select--", new { @class = "form-control" })
                                        @Html.ValidationMessageFor(m => m.BodyTypeID)
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="color" class="col-sm-4 control-label">@Html.DisplayNameFor(m => m.Color)</label>
                                    <div class="col-sm-8">
                                        @Html.TextBoxFor(m => m.Color, new { @class = "form-control" })
                                        @Html.ValidationMessageFor(m=>m.Color)
                                        
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="MOT_ed" class="col-sm-4 control-label">@Html.DisplayNameFor(m => m.MOTExpiryDate)</label>
                                    <div class="col-sm-8 datewrap">
                                        <span class="fa fa-calendar"></span>

                                        @Html.TextBoxFor(m => m.MOTExpiryDate, new { @class = "form-control", placeholder = "Enter MOT Expiry Date" })
                                        @Html.ValidationMessageFor(m => m.MOTExpiryDate)
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="inputPassword3" class="col-sm-4 control-label">@Html.DisplayNameFor(m => m.TransmissionTypeID)</label>
                                    <div class="col-sm-8">

                                        @Html.DropDownList("TransmissionTypeID", new SelectList(Model.TransmissionTypeList, "ID", "Type"), "--Select--", new { @class = "form-control" })
                                        @Html.ValidationMessageFor(m => m.TransmissionTypeID)

                                    </div>
                                </div>



                                <div class="form-group">
                                    <label for="mileage" class="col-sm-4 control-label">@Html.DisplayNameFor(m => m.ExactMileage)</label>
                                    <div class="col-sm-8">

                                        @Html.TextBoxFor(m => m.ExactMileage, new { @class = "form-control" })
                                        @Html.ValidationMessageFor(m => m.ExactMileage)
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="interiorColor" class="col-md-4 control-label">@Html.DisplayNameFor(m => m.InteriorColor)</label>
                                    <div class="col-md-4">

                                        @Html.TextBoxFor(m => m.InteriorColor, new { @class = "form-control" })
                                        @Html.ValidationMessageFor(m => m.InteriorColor)
                                    </div>
                                    <div class="col-md-4">
                                     @Html.TextBoxFor(m => m.Trim, new { @class = "form-control" }) 
                                    
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-md-4 control-label">Fuel</label>
                                    <div class="col-sm-8" id="divFuelTypes">

                                        @for (int i = 0; i < Model.FuelTypeList.Count; i++)
                                        {
                                            @Html.CheckBoxFor(m => m.FuelTypeList[i].IsChecked, new { id = "FuelTypeList_" + i, @style = "margin-right:5px;margin-left:5px;" })
                                            @Html.HiddenFor(m => m.FuelTypeList[i].ID)
                                            <span style="font-weight:bold;">
                                                @Html.DisplayFor(m => m.FuelTypeList[i].Name)
                                            </span>
                                        }
                                        <span id="spanfuelmsg"></span>

                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="color" class="col-sm-4 control-label">@Html.DisplayNameFor(m => m.EngineSize)</label>
                                    <div class="col-sm-8">
                                        @Html.TextBoxFor(m => m.EngineSize, new { @class = "form-control" })
                                        @Html.ValidationMessageFor(m => m.EngineSize)

                                    </div>
                                </div>





                                <div class="form-group">
                                    <label for="Tax_ed" class="col-sm-4 control-label">@Html.DisplayNameFor(m => m.TAXExpiryDate)</label>
                                    <div class="col-sm-8 datewrap">
                                        <span class="fa fa-calendar"></span>

                                        @Html.TextBoxFor(m => m.TAXExpiryDate, new { @class = "form-control" })
                                        @Html.ValidationMessageFor(m => m.TAXExpiryDate)
                                    </div>


                                </div>

                                <div class="form-group">
                                    <label for="Tax_ed" class="col-sm-4 control-label">@Html.DisplayNameFor(m => m.DateOfFirstRegistration)</label>
                                    <div class="col-sm-8 datewrap">
                                        <span class="fa fa-calendar"></span>

                                        @Html.TextBoxFor(m => m.DateOfFirstRegistration, new { @class = "form-control" })
                                        @Html.ValidationMessageFor(m => m.DateOfFirstRegistration)
                                    </div>


                                </div>

                                <div class="form-group">
                                    <label for="Tax_ed" class="col-sm-4 control-label">@Html.DisplayNameFor(m => m.ServiceHistory)</label>
                                    <div class="col-sm-8 datewrap">
                                        @Html.TextAreaFor(m => m.ServiceHistory, new { @class = "form-control" })
                                        @Html.ValidationMessageFor(m => m.ServiceHistory)
                                    </div>
                                </div>
                            </div>
