@model AutoBid.Models.Auction.AuctionHouseAddEditVehicleModel
@using ctrlr = AutoBid.Controllers.Auction.AuctionHouseAddEditVehicleController
@using AutoBid.Enums;

@{
    ViewBag.Title = "Auction House Car Vehicle";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="modal fade" id="imageUploadModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-input">
        <div class="modal-content">
            <div class="jumbotron">
                <form action="~/AuctionHouseAddEditVehicle/UploadImage/" method="post" enctype="multipart/form-data" class="dropzone" id="dropzoneForm">
                    <div class="fallback">

                        <input name="file" type="file" multiple accept="image/*" />
                        <input type="submit" value="Upload" />
                    </div>
                </form>
            </div>
        </div>
    </div>

</div>



<div id="auctionSale-pop-up" class="modal fade " role="dialog">
    <div class="modal-dialog" style="width:70%">
        <div class="modal-content ">
            <div class="modal-header bg-blue-soft ">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title"><b>Auction House Sale</b></h4>
            </div>
            <form id="frmAucSaleNew" method="post" action="/AuctionHouseAddEditVehicle/AddNewAuctionHouse" novalidate="novalidate">
                <div class="modal-body ">
                    <div id="msg" style="display:none;"></div>
                    <div id="content"></div>
                    <div class="row">

                        <div class="col-md-8">

                            <div class="form-group">
                                <label class="col-sm-4 control-label" for="inputDate">Sale Date</label>
                                <div class="col-sm-8 datewrap">
                                    <span class="fa fa-calendar"></span>
                                    @Html.TextBoxFor(m => m.NewSaleDate, new { @class = "form-control", placeholder = "Enter Sale Date", id = "SaleDate" })

                                </div>
                            </div>
                            <br />
                            <div class="form-group">
                                <label class="col-sm-4 control-label" for="title">Title</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(m => m.Title, new { @class = "form-control ", placeholder = "Title" })
                                </div>
                                <div id="divSaleTitle" style="color:red;display:none;"></div>
                            </div>

                        </div>

                        <div class="col-md-4">

                        </div>

                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" id="AddAucSale" class="btn btn-success ladda-button" data-style="expand-left">Add</button>
                    <button type="button" id="AddAucSaleCan" class="btn btn-success ladda-button" data-style="expand-left">Cancel</button>
                </div>
            </form>
        </div>

    </div>
</div>
<div class="section formwrap">
